  CREATE OR REPLACE EDITIONABLE PACKAGE BODY "C##LENGUAJES"."PKG_PRODUCTOS" AS

    PROCEDURE obtener_productos (
        p_cursor OUT SYS_REFCURSOR
    ) IS
    BEGIN
        OPEN p_cursor FOR SELECT
            id_producto,
            nombre,
            descripcion,
            precio,
            disponibilidad,
            id_categoria
        FROM 
            productos;
    END;

    PROCEDURE eliminar_producto (
        p_id_producto IN NUMBER
    ) IS 
    BEGIN
        DELETE FROM productos
        WHERE
            id_producto = p_id_producto;
    END;

    PROCEDURE obtener_producto_por_id (
        p_id IN NUMBER,
        p_cursor OUT SYS_REFCURSOR
    ) IS
    BEGIN
        OPEN p_cursor FOR SELECT * FROM productos WHERE id_producto = p_id;
    END;

    PROCEDURE actualizar_producto (
        p_id_producto IN NUMBER,
        p_nombre IN VARCHAR2,
        p_descripcion IN VARCHAR2,
        p_precio IN NUMBER,
        p_disponibilidad IN CHAR,
        p_id_categoria IN NUMBER
    ) IS
    BEGIN
        UPDATE productos
        SET nombre = p_nombre,
            descripcion = p_descripcion,
            precio = p_precio,
            disponibilidad = p_disponibilidad,
            id_categoria = p_id_categoria
        WHERE id_producto = p_id_producto;
    END;

    PROCEDURE insertar_producto (
        p_nombre IN VARCHAR2,
        p_descripcion IN VARCHAR2,
        p_precio IN NUMBER,
        p_disponibilidad IN CHAR,
        p_id_categoria IN NUMBER
    ) IS
    BEGIN
        INSERT INTO productos (nombre, descripcion, precio, disponibilidad,
        id_categoria)
        VALUES (p_nombre, p_descripcion, p_precio, p_disponibilidad, p_id_categoria);
    END;
END pkg_productos;

