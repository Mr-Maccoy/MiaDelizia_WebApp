  CREATE OR REPLACE EDITIONABLE FUNCTION "C##LENGUAJES"."FN_ESTADO_PEDIDO" (p_id_pedido NUMBER)
RETURN VARCHAR2 IS
  v_estado VARCHAR2(50);
BEGIN
  SELECT estado_pedido INTO v_estado FROM pedidos WHERE id_pedido = p_id_pedido;
  RETURN v_estado;
END;

