RETURN VARCHAR2 IS
  v_nombre VARCHAR2(100);
BEGIN
  SELECT nombre INTO v_nombre FROM productos WHERE id_producto = p_id_producto;
  RETURN v_nombre;
END;

/
--------------------------------------------------------
--  DDL for Function FN_OBTENER_DESCUENTO
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "C##LENGUAJES"."FN_OBTENER_DESCUENTO" (p_id_cliente NUMBER)
RETURN NUMBER IS
  v_tipo VARCHAR2(20);
BEGIN
  SELECT tipo_cliente INTO v_tipo FROM clientes WHERE id_cliente = p_id_cliente;

  RETURN CASE 
    WHEN v_tipo = 'VIP' THEN 15
    WHEN v_tipo = 'recurrente' THEN 10
    ELSE 0
  END;
END;

/
--------------------------------------------------------
--  DDL for Function FN_PROMEDIO_GASTO_CLIENTE
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "C##LENGUAJES"."FN_PROMEDIO_GASTO_CLIENTE" (p_id_cliente NUMBER)
RETURN NUMBER IS
  v_promedio NUMBER;
BEGIN
  SELECT AVG(monto_total) INTO v_promedio FROM pedidos WHERE id_cliente = p_id_cliente;
  RETURN NVL(v_promedio, 0);
END;


/
--------------------------------------------------------
--  DDL for Function FN_TOTAL_COMPRAS_CLIENTE
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "C##LENGUAJES"."FN_TOTAL_COMPRAS_CLIENTE" (p_id_cliente NUMBER)
RETURN NUMBER IS
  v_total NUMBER;
BEGIN
  SELECT SUM(monto_total) INTO v_total
  FROM pedidos
  WHERE id_cliente = p_id_cliente;

  RETURN NVL(v_total, 0);
END;

/
--------------------------------------------------------
--  DDL for Function FN_ULTIMA_COMPRA_CLIENTE
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "C##LENGUAJES"."FN_ULTIMA_COMPRA_CLIENTE" (p_id_cliente NUMBER)
RETURN DATE IS
  v_fecha DATE;
BEGIN
  SELECT MAX(fecha_pedido) INTO v_fecha FROM pedidos WHERE id_cliente = p_id_cliente;
  RETURN v_fecha;
END;

/
--------------------------------------------------------
--  DDL for Function FN_VALIDAR_TELEFONO
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "C##LENGUAJES"."FN_VALIDAR_TELEFONO" (p_telefono VARCHAR2)
RETURN VARCHAR2 IS
BEGIN
    IF REGEXP_LIKE(p_telefono, '^[2678][0-9]{7}$') THEN
        RETURN 'VÁLIDO';
    ELSE
        RETURN 'NO VÁLIDO';
    END IF;
END;

/
--------------------------------------------------------
--  DDL for Function FN_VERIFICAR_DISPONIBILIDAD
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "C##LENGUAJES"."FN_VERIFICAR_DISPONIBILIDAD" (p_id_producto NUMBER, p_cantidad_solicitada NUMBER)
RETURN VARCHAR2 IS
    v_disponible NUMBER;
BEGIN
    SELECT cantidad_disponible
    INTO v_disponible
    FROM INVENTARIO
    WHERE id_producto = p_id_producto;

    IF v_disponible >= p_cantidad_solicitada THEN
        RETURN 'DISPONIBLE';
    ELSE
        RETURN 'NO DISPONIBLE';
    END IF;

EXCEPTION
    WHEN NO_DATA_FOUND THEN
        RETURN 'PRODUCTO NO EXISTE';
END;

/
