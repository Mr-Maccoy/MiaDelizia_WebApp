  CREATE OR REPLACE EDITIONABLE PACKAGE BODY "C##LENGUAJES"."PKG_AUDITORIA" AS
  PROCEDURE obtener_auditoria(p_cursor OUT SYS_REFCURSOR) IS
  BEGIN
    OPEN p_cursor FOR
    SELECT id_auditoria,
           tabla,
           operacion,
           id_registro,
           usuario,
           fecha_operacion,
           detalle
    FROM auditoria
    ORDER BY fecha_operacion DESC;
  END;
END pkg_auditoria;

