--------------------------------------------------------
--  DDL for Trigger TRG_ACTUALIZAR_INVENTARIO
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "C##LENGUAJES"."TRG_ACTUALIZAR_INVENTARIO" 
AFTER INSERT ON detalle_pedido
FOR EACH ROW
BEGIN
    UPDATE inventario
    SET cantidad_disponible = cantidad_disponible - :NEW.cantidad
    WHERE id_producto = :NEW.id_producto;
END;
/
ALTER TRIGGER "C##LENGUAJES"."TRG_ACTUALIZAR_INVENTARIO" ENABLE;
--------------------------------------------------------
--  DDL for Trigger TRG_AUDITORIA_CLIENTES
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "C##LENGUAJES"."TRG_AUDITORIA_CLIENTES" 
AFTER INSERT OR UPDATE OR DELETE ON clientes
FOR EACH ROW
DECLARE
    v_operacion   VARCHAR2(10);
    v_id_registro NUMBER;
    v_detalle     VARCHAR2(4000);
BEGIN
    IF INSERTING THEN
        v_operacion := 'INSERT';
        v_id_registro := :NEW.id_cliente;
        v_detalle := 'Nuevo cliente: ' || :NEW.nombre;
    ELSIF UPDATING THEN
        v_operacion := 'UPDATE';
        v_id_registro := :OLD.id_cliente;
        v_detalle := 'Cliente actualizado: ' || :NEW.nombre;
    ELSIF DELETING THEN
        v_operacion := 'DELETE';
        v_id_registro := :OLD.id_cliente;
        v_detalle := 'Cliente eliminado: ' || :OLD.nombre;
    END IF;

    -- Insertar en tabla de auditoría
    INSERT INTO auditoria (
        tabla, operacion, id_registro, usuario, detalle
    ) VALUES (
        'clientes', v_operacion, v_id_registro, USER, v_detalle
    );
END;
/
ALTER TRIGGER "C##LENGUAJES"."TRG_AUDITORIA_CLIENTES" ENABLE;
--------------------------------------------------------
