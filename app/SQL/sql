--  DDL for Trigger TRG_AUDITORIA_PRODUCTOS
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "C##LENGUAJES"."TRG_AUDITORIA_PRODUCTOS" 
AFTER INSERT OR UPDATE OR DELETE ON productos
FOR EACH ROW
DECLARE
    v_operacion   VARCHAR2(10);
    v_id_registro NUMBER;
    v_detalle     VARCHAR2(4000);
BEGIN
    IF INSERTING THEN
        v_operacion := 'INSERT';
        v_id_registro := :NEW.id_producto;
        v_detalle := 'Nuevo producto: ' || :NEW.nombre;
    ELSIF UPDATING THEN
        v_operacion := 'UPDATE';
        v_id_registro := :OLD.id_producto;
        v_detalle := 'Producto actualizado: ' || :NEW.nombre;
    ELSIF DELETING THEN
        v_operacion := 'DELETE';
        v_id_registro := :OLD.id_producto;
        v_detalle := 'Producto eliminado: ' || :OLD.nombre;
    END IF;

    -- Insertar en auditoría
    INSERT INTO auditoria (
        tabla, operacion, id_registro, usuario, detalle
    ) VALUES (
        'productos', v_operacion, v_id_registro, USER, v_detalle
    );
END;
/
ALTER TRIGGER "C##LENGUAJES"."TRG_AUDITORIA_PRODUCTOS" ENABLE;
