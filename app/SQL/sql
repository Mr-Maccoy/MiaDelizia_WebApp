--------------------------------------------------------
--  DDL for Package Body PKG_CATEGORIAS
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE PACKAGE BODY "C##LENGUAJES"."PKG_CATEGORIAS" AS
 
  PROCEDURE insertar_categoria(p_nombre IN VARCHAR2, p_descripcion IN VARCHAR2) IS
  BEGIN
    INSERT INTO categorias (nombre_categoria, descripcion_categoria)
    VALUES (p_nombre, p_descripcion);
  END;

  PROCEDURE actualizar_categoria(p_id IN NUMBER, p_nombre IN VARCHAR2, p_descripcion IN VARCHAR2) IS
  BEGIN
    UPDATE categorias
    SET nombre_categoria = p_nombre,
        descripcion_categoria = p_descripcion
    WHERE id_categoria = p_id;
  END;

  PROCEDURE eliminar_categoria(p_id IN NUMBER) IS
  BEGIN
    DELETE FROM categorias WHERE id_categoria = p_id;
  END;

  PROCEDURE obtener_categorias(p_cursor OUT SYS_REFCURSOR) IS
  BEGIN
    OPEN p_cursor FOR
    SELECT id_categoria, nombre_categoria, descripcion_categoria
    FROM categorias
    ORDER BY nombre_categoria;
  END;

  PROCEDURE obtener_categoria_por_id(p_id IN NUMBER, p_cursor OUT SYS_REFCURSOR) IS
  BEGIN
    OPEN p_cursor FOR
    SELECT id_categoria, nombre_categoria, descripcion_categoria
    FROM categorias
    WHERE id_categoria = p_id;
  END;

END pkg_categorias;

/
--------------------------------------------------------
--  DDL for Package Body PKG_CLIENTES
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE PACKAGE BODY "C##LENGUAJES"."PKG_CLIENTES" AS

    PROCEDURE obtener_clientes (
        p_cursor OUT SYS_REFCURSOR
    ) IS
    BEGIN
        OPEN p_cursor FOR SELECT
                              id_cliente,
                              nombre,
                              telefono,
                              correo_electronico,
                              direccion,
                              tipo_cliente,
                              fecha_registro_cliente,
                              fecha_nacimiento
                          FROM
                              clientes;

    END;

    PROCEDURE eliminar_clientes (
        p_id_cliente IN NUMBER
    ) IS
    BEGIN
        DELETE FROM clientes
        WHERE
            id_cliente = p_id_cliente;

    END;

PROCEDURE obtener_cliente_por_id (
    p_id     IN NUMBER,
    p_cursor OUT SYS_REFCURSOR
)is
BEGIN
    OPEN p_cursor FOR SELECT * FROM clientes WHERE id_cliente = p_id;

    end;

    PROCEDURE actualizar_cliente(
    p_id_cliente IN NUMBER,
    p_nombre IN VARCHAR2,
    p_telefono IN VARCHAR2,
    p_correo IN VARCHAR2,
    p_direccion IN VARCHAR2,
    p_tipo IN VARCHAR2,
    p_nacimiento IN DATE
) IS
BEGIN
    UPDATE CLIENTES
    SET NOMBRE = p_nombre,
        TELEFONO = p_telefono,
        CORREO_ELECTRONICO = p_correo,
        DIRECCION = p_direccion,
        TIPO_CLIENTE = p_tipo,
        FECHA_NACIMIENTO = p_nacimiento
    WHERE ID_CLIENTE = p_id_cliente;
END;

PROCEDURE insertar_cliente(
    p_nombre IN VARCHAR2,
    p_telefono IN VARCHAR2,
    p_correo IN VARCHAR2,
    p_direccion IN VARCHAR2,
    p_tipo IN VARCHAR2,
    p_nacimiento IN DATE
) IS
BEGIN
    INSERT INTO CLIENTES (NOMBRE, TELEFONO, CORREO_ELECTRONICO, DIRECCION, TIPO_CLIENTE, FECHA_NACIMIENTO)
    VALUES (p_nombre, p_telefono, p_correo, p_direccion, p_tipo, p_nacimiento);
END;

END pkg_clientes;

/
--------------------------------------------------------
--  DDL for Package Body PKG_DETALLE_PEDIDO
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE PACKAGE BODY "C##LENGUAJES"."PKG_DETALLE_PEDIDO" AS
 
  PROCEDURE agregar_detalle(

    p_id_pedido IN NUMBER,

    p_id_producto IN NUMBER,

    p_cantidad IN NUMBER,

    p_precio_unitario IN NUMBER

  ) IS

  BEGIN

    INSERT INTO detalle_pedido(id_pedido, id_producto, cantidad, precio_unitario)

    VALUES (p_id_pedido, p_id_producto, p_cantidad, p_precio_unitario);

  END;

  PROCEDURE actualizar_detalle(

    p_id_detalle IN NUMBER,

    p_id_pedido IN NUMBER,

    p_id_producto IN NUMBER,

    p_cantidad IN NUMBER,

    p_precio_unitario IN NUMBER

  ) IS

  BEGIN

    UPDATE detalle_pedido

    SET id_pedido = p_id_pedido,

        id_producto = p_id_producto,

        cantidad = p_cantidad,

        precio_unitario = p_precio_unitario

    WHERE id_detalle = p_id_detalle;

  END;

  PROCEDURE eliminar_detalle(p_id_detalle IN NUMBER) IS

  BEGIN

    DELETE FROM detalle_pedido WHERE id_detalle = p_id_detalle;

  END;

  PROCEDURE obtener_detalles(p_cursor OUT SYS_REFCURSOR) IS

  BEGIN

    OPEN p_cursor FOR

    SELECT id_detalle,

           id_pedido,

           id_producto,

           cantidad,

           precio_unitario,

           subtotal

    FROM detalle_pedido; 



  END;

  PROCEDURE obtener_detalle_por_id(p_id_detalle IN NUMBER, p_cursor OUT SYS_REFCURSOR) IS

  BEGIN

    OPEN p_cursor FOR

    SELECT d.id_detalle,

           d.id_pedido,

           d.id_producto,

           p.nombre AS nombre_producto,

           d.cantidad,

           d.precio_unitario,

           d.subtotal

    FROM detalle_pedido d

    JOIN productos p ON d.id_producto = p.id_producto

    WHERE d.id_detalle = p_id_detalle;

  END;

END pkg_detalle_pedido;

/
--------------------------------------------------------
--  DDL for Package Body PKG_EVENTOS
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE PACKAGE BODY "C##LENGUAJES"."PKG_EVENTOS" AS

    PROCEDURE obtener_eventos (
    e_cursor OUT SYS_REFCURSOR
    ) IS
    BEGIN
    OPEN e_cursor FOR
    SELECT id_evento, nombre_evento, fecha_evento, ubicacion, descripcion
    FROM eventos;
    END obtener_eventos;

    PROCEDURE eliminar_evento (
    e_id_evento IN NUMBER
    ) IS
    BEGIN
    DELETE FROM eventos
    WHERE id_evento = e_id_evento;
    END eliminar_evento;

    PROCEDURE obtener_evento_por_id (
    e_id     IN NUMBER,
    e_cursor OUT SYS_REFCURSOR
    ) IS
    BEGIN
        OPEN e_cursor FOR
        SELECT *
        FROM eventos
        WHERE id_evento = e_id;
    END obtener_evento_por_id;

    PROCEDURE actualizar_evento (
    e_id_evento   IN NUMBER,
    e_nombre      IN VARCHAR2,
    e_fecha       IN DATE,
    e_ubicacion   IN VARCHAR2,
    e_descripcion IN VARCHAR2
    ) IS
    BEGIN
        UPDATE eventos
        SET nombre_evento = e_nombre,
        fecha_evento  = e_fecha,
        ubicacion     = e_ubicacion,
        descripcion   = e_descripcion
        WHERE id_evento = e_id_evento;
    END actualizar_evento;

    PROCEDURE insertar_evento (
    e_nombre      IN VARCHAR2,
    e_fecha       IN DATE,
    e_ubicacion   IN VARCHAR2,
    e_descripcion IN VARCHAR2
    ) IS
    BEGIN
    INSERT INTO eventos (nombre_evento, fecha_evento, ubicacion, descripcion)
    VALUES (e_nombre, e_fecha, e_ubicacion, e_descripcion);
    END insertar_evento;

END pkg_eventos;

/
--------------------------------------------------------
--  DDL for Package Body PKG_FACTURAS
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE PACKAGE BODY "C##LENGUAJES"."PKG_FACTURAS" AS
 
  PROCEDURE obtener_facturas(f_cursor OUT SYS_REFCURSOR) IS
  BEGIN
    OPEN f_cursor FOR
    SELECT id_factura, id_pedido, fecha_factura, total, estado_factura
    FROM factura
    ORDER BY fecha_factura DESC;
  END;

  PROCEDURE insertar_factura(p_id_pedido IN NUMBER, p_total IN NUMBER, p_estado IN VARCHAR2) IS
  BEGIN
    INSERT INTO factura (id_pedido, total, estado_factura)
    VALUES (p_id_pedido, p_total, p_estado);
  END;

  PROCEDURE actualizar_factura(p_id IN NUMBER, p_total IN NUMBER, p_estado IN VARCHAR2) IS
  BEGIN
    UPDATE factura
    SET total = p_total,
        estado_factura = p_estado
    WHERE id_factura = p_id;
  END;

  PROCEDURE eliminar_factura(p_id IN NUMBER) IS
  BEGIN
    DELETE FROM factura WHERE id_factura = p_id;
  END;

  PROCEDURE obtener_factura_por_id(p_id IN NUMBER, f_cursor OUT SYS_REFCURSOR) IS
  BEGIN
    OPEN f_cursor FOR
    SELECT id_factura, id_pedido, fecha_factura, total, estado_factura
    FROM factura
    WHERE id_factura = p_id;
  END;

END pkg_facturas;

/
--------------------------------------------------------
--  DDL for Package Body PKG_HISTORIAL_ESTADO
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE PACKAGE BODY "C##LENGUAJES"."PKG_HISTORIAL_ESTADO" AS
 
  PROCEDURE insertar_historial(
    p_id_pedido IN NUMBER,
    p_estado IN VARCHAR2
  ) IS
  BEGIN
    INSERT INTO historial_estado_pedido(id_pedido, estado)
    VALUES (p_id_pedido, p_estado);
  END;

  PROCEDURE actualizar_historial(
    p_id_historial IN NUMBER,
    p_estado IN VARCHAR2
  ) IS
  BEGIN
    UPDATE historial_estado_pedido
    SET estado = p_estado,
        fecha_estado = SYSDATE
    WHERE id_historial = p_id_historial;
  END;

  PROCEDURE eliminar_historial(p_id_historial IN NUMBER) IS
  BEGIN
    DELETE FROM historial_estado_pedido WHERE id_historial = p_id_historial;
  END;

  PROCEDURE obtener_historiales(p_cursor OUT SYS_REFCURSOR) IS
  BEGIN
    OPEN p_cursor FOR
    SELECT h.id_historial,
           h.id_pedido,
           h.estado,
           h.fecha_estado
    FROM historial_estado_pedido h;
  END;

  PROCEDURE obtener_historial_por_id(p_id_historial IN NUMBER, p_cursor OUT SYS_REFCURSOR) IS
  BEGIN
    OPEN p_cursor FOR
    SELECT h.id_historial,
           h.id_pedido,
           h.estado,
           h.fecha_estado
    FROM historial_estado_pedido h
    WHERE h.id_historial = p_id_historial;
  END;

END pkg_historial_estado;

/
--------------------------------------------------------
--  DDL for Package Body PKG_INVENTARIO
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE PACKAGE BODY "C##LENGUAJES"."PKG_INVENTARIO" AS
    PROCEDURE obtener_inventario (
    i_cursor OUT SYS_REFCURSOR
    ) IS
    BEGIN
    OPEN i_cursor FOR
    SELECT id_inventario, id_producto, cantidad_disponible, fecha_actualizacion
    FROM inventario;
END obtener_inventario;

    PROCEDURE eliminar_inventario (
    i_id_inventario IN NUMBER
    ) IS
    BEGIN
    DELETE FROM inventario
    WHERE id_inventario = i_id_inventario;
    END eliminar_inventario;

    PROCEDURE obtener_inventario_por_id (
    i_id     IN NUMBER,
    i_cursor OUT SYS_REFCURSOR
    ) IS
    BEGIN
    OPEN i_cursor FOR
    SELECT *
    FROM inventario
    WHERE id_inventario = i_id;
    END obtener_inventario_por_id;

    PROCEDURE actualizar_inventario (
    i_id_inventario       IN NUMBER,
    i_id_producto         IN NUMBER,
    i_cantidad_disponible IN NUMBER,
    i_fecha_actualizacion IN DATE
    ) IS
    BEGIN
    UPDATE inventario
    SET id_producto         = i_id_producto,
        cantidad_disponible = i_cantidad_disponible,
        fecha_actualizacion = i_fecha_actualizacion
    WHERE id_inventario = i_id_inventario;
    END actualizar_inventario;

    PROCEDURE insertar_inventario (
    i_id_producto         IN NUMBER,
    i_cantidad_disponible IN NUMBER,
    i_fecha_actualizacion IN DATE
    ) IS
    BEGIN
        INSERT INTO inventario (
        id_producto,
        cantidad_disponible,
        fecha_actualizacion
        ) VALUES (
        i_id_producto,
        i_cantidad_disponible,
        i_fecha_actualizacion
        );
    END insertar_inventario;

END pkg_inventario;

/
--------------------------------------------------------
--  DDL for Package Body PKG_PAGOS
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE PACKAGE BODY "C##LENGUAJES"."PKG_PAGOS" AS
 
  PROCEDURE insertar_pago(
    p_id_pedido IN NUMBER,
    p_monto IN NUMBER,
    p_metodo_pago IN VARCHAR2,
    p_estado_pago IN VARCHAR2
  ) IS
  BEGIN
    INSERT INTO pagos (id_pedido, monto, metodo_pago, estado_pago)
    VALUES (p_id_pedido, p_monto, p_metodo_pago, p_estado_pago);
  END;

  PROCEDURE actualizar_pago(
    p_id_pago IN NUMBER,
    p_monto IN NUMBER,
    p_metodo_pago IN VARCHAR2,
    p_estado_pago IN VARCHAR2
  ) IS
  BEGIN
    UPDATE pagos
    SET monto = p_monto,
        metodo_pago = p_metodo_pago,
        estado_pago = p_estado_pago
    WHERE id_pago = p_id_pago;
  END;

  PROCEDURE eliminar_pago(p_id_pago IN NUMBER) IS
  BEGIN
    DELETE FROM pagos WHERE id_pago = p_id_pago;
  END;

  PROCEDURE obtener_pagos(p_cursor OUT SYS_REFCURSOR) IS
  BEGIN
    OPEN p_cursor FOR
    SELECT id_pago, id_pedido, fecha_pago, monto, metodo_pago, estado_pago
    FROM pagos
    ORDER BY fecha_pago DESC;
  END;

  PROCEDURE obtener_pago_por_id(p_id_pago IN NUMBER, p_cursor OUT SYS_REFCURSOR) IS
  BEGIN
    OPEN p_cursor FOR
    SELECT id_pago, id_pedido, fecha_pago, monto, metodo_pago, estado_pago
    FROM pagos
    WHERE id_pago = p_id_pago;
  END;

END pkg_pagos;

/
--------------------------------------------------------
--  DDL for Package Body PKG_PEDIDOS
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE PACKAGE BODY "C##LENGUAJES"."PKG_PEDIDOS" AS

    PROCEDURE obtener_pedidos (
        p_cursor OUT SYS_REFCURSOR
    ) IS
    BEGIN
        OPEN p_cursor FOR SELECT
            id_pedido,
            id_cliente,
            fecha_pedido,
            estado_pedido,
            fecha_entrega,
            tipo_envio,
            monto_total
        FROM 
            pedidos;
    END;

    PROCEDURE eliminar_pedido (
        p_id_pedido IN NUMBER
    ) IS 
    BEGIN
        DELETE FROM pedidos
        WHERE
            id_pedido = p_id_pedido;
    END;

    PROCEDURE obtener_pedido_por_id (
        p_id IN NUMBER,
        p_cursor OUT SYS_REFCURSOR
    ) IS
    BEGIN
        OPEN p_cursor FOR SELECT * FROM pedidos WHERE id_pedido = p_id;

    END;

    PROCEDURE actualizar_pedido (
        p_id_pedido IN NUMBER,
        p_id_cliente IN NUMBER,
        p_estado_pedido IN VARCHAR2,
        p_fecha_entrega IN DATE,
        p_tipo_envio IN VARCHAR2,
        p_monto_total IN NUMBER
    ) IS
    BEGIN
        UPDATE pedidos
        SET id_cliente = p_id_cliente,
            estado_pedido = p_estado_pedido,
            fecha_entrega = p_fecha_entrega,
            tipo_envio = p_tipo_envio,
            monto_total = p_monto_total
        WHERE id_pedido = p_id_pedido;
    END;

    PROCEDURE insertar_pedido (
        p_id_cliente IN NUMBER,
        p_estado_pedido IN VARCHAR2,
        p_fecha_entrega IN DATE,
        p_tipo_envio IN VARCHAR2,
        p_monto_total IN NUMBER
    ) IS 
    BEGIN
        INSERT INTO pedidos (id_cliente,estado_pedido,fecha_entrega,
        tipo_envio, monto_total)
        VALUES (p_id_cliente, p_estado_pedido, p_fecha_entrega, p_tipo_envio, p_monto_total);

    END;

END pkg_pedidos;

