RETURN VARCHAR2 IS
  v_nombre VARCHAR2(100);
BEGIN
  SELECT nombre INTO v_nombre FROM clientes WHERE id_cliente = p_id_cliente;
  RETURN v_nombre; -- Agrega apellidos si tu tabla los tiene
END;

