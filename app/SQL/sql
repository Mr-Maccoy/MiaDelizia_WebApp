--------------------------------------------------------
--  DDL for Trigger TRG_NOTIFICACION_EVENTO
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE TRIGGER "C##LENGUAJES"."TRG_NOTIFICACION_EVENTO" 
AFTER INSERT ON eventos
FOR EACH ROW
DECLARE
    v_nombre_evento VARCHAR2(100);
    v_fecha_evento DATE;
BEGIN
    v_nombre_evento := :NEW.nombre_evento;
    v_fecha_evento := :NEW.fecha_evento;

    DBMS_OUTPUT.PUT_LINE('Nuevo evento: ' ||  v_nombre_evento || 'en la fecha: ' || TO_CHAR(v_fecha_evento, 'DD-MM-YYYY'));
END;
/
ALTER TRIGGER "C##LENGUAJES"."TRG_NOTIFICACION_EVENTO" ENABLE;
