--------------------------------------------------------
--  DDL for Package Body PKG_ROLES
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE PACKAGE BODY "C##LENGUAJES"."PKG_ROLES" AS
 
  PROCEDURE insertar_rol(p_nombre_rol IN VARCHAR2) IS
  BEGIN
    INSERT INTO roles(nombre_rol) VALUES (p_nombre_rol);
  END;

  PROCEDURE actualizar_rol(p_id_rol IN NUMBER, p_nombre_rol IN VARCHAR2) IS
  BEGIN
    UPDATE roles
    SET nombre_rol = p_nombre_rol
    WHERE id_rol = p_id_rol;
  END;

  PROCEDURE eliminar_rol(p_id_rol IN NUMBER) IS
  BEGIN
    DELETE FROM roles WHERE id_rol = p_id_rol;
  END;

  PROCEDURE obtener_roles(p_cursor OUT SYS_REFCURSOR) IS
  BEGIN
    OPEN p_cursor FOR
    SELECT id_rol, nombre_rol FROM roles;
  END;

  PROCEDURE obtener_rol_por_id(p_id_rol IN NUMBER, p_cursor OUT SYS_REFCURSOR) IS
  BEGIN
    OPEN p_cursor FOR
    SELECT id_rol, nombre_rol FROM roles
    WHERE id_rol = p_id_rol;
  END;

END pkg_roles;

/
--------------------------------------------------------
--  DDL for Package Body PKG_USUARIOS
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE PACKAGE BODY "C##LENGUAJES"."PKG_USUARIOS" AS
 
  PROCEDURE insertar_usuario(
    p_id_rol IN NUMBER,
    p_nombre_usuario IN VARCHAR2,
    p_clave IN VARCHAR2,
    p_email IN VARCHAR2
  ) IS
  BEGIN
    INSERT INTO usuarios (id_rol, nombre_usuario, clave, email)
    VALUES (p_id_rol, p_nombre_usuario, p_clave, p_email);
  END;

  PROCEDURE actualizar_usuario(
    p_id_usuario IN NUMBER,
    p_id_rol IN NUMBER,
    p_nombre_usuario IN VARCHAR2,
    p_clave IN VARCHAR2,
    p_email IN VARCHAR2
  ) IS
  BEGIN
    UPDATE usuarios
    SET id_rol = p_id_rol,
        nombre_usuario = p_nombre_usuario,
        clave = p_clave,
        email = p_email,
        fecha_modificacion = SYSDATE
    WHERE id_usuario = p_id_usuario;
  END;

  PROCEDURE eliminar_usuario(p_id_usuario IN NUMBER) IS
  BEGIN
    DELETE FROM usuarios WHERE id_usuario = p_id_usuario;
  END;

  PROCEDURE obtener_usuarios(p_cursor OUT SYS_REFCURSOR) IS
  BEGIN
    OPEN p_cursor FOR
    SELECT u.id_usuario,
           u.id_rol,
           r.nombre_rol,
           u.nombre_usuario,
           u.email,
           u.fecha_creacion,
           u.fecha_modificacion
    FROM usuarios u
    JOIN roles r ON u.id_rol = r.id_rol;
  END;

  PROCEDURE obtener_usuario_por_id(p_id_usuario IN NUMBER, p_cursor OUT SYS_REFCURSOR) IS
  BEGIN
    OPEN p_cursor FOR
    SELECT id_usuario, id_rol, nombre_usuario, clave, email, fecha_creacion, fecha_modificacion
    FROM usuarios
    WHERE id_usuario = p_id_usuario;
  END;

END pkg_usuarios;

/
--------------------------------------------------------
--  DDL for Function FN_CALCULAR_EDAD
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "C##LENGUAJES"."FN_CALCULAR_EDAD" (p_fecha_nacimiento DATE)
RETURN NUMBER IS
BEGIN
  RETURN TRUNC(MONTHS_BETWEEN(SYSDATE, p_fecha_nacimiento) / 12);
END;

/
--------------------------------------------------------
--  DDL for Function FN_CALCULAR_IVA
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "C##LENGUAJES"."FN_CALCULAR_IVA" (p_monto NUMBER)
RETURN NUMBER IS
BEGIN
  RETURN p_monto * 0.13;
END;

/
--------------------------------------------------------
--  DDL for Function FN_CANTIDAD_COMPRAS_CLIENTE
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "C##LENGUAJES"."FN_CANTIDAD_COMPRAS_CLIENTE" (p_id_cliente NUMBER)
RETURN NUMBER IS
  v_cantidad NUMBER;
BEGIN
  SELECT COUNT(*) INTO v_cantidad FROM pedidos WHERE id_cliente = p_id_cliente;
  RETURN v_cantidad;
END;

/
--------------------------------------------------------
--  DDL for Function FN_CANTIDAD_PRODUCTOS_EN_STOCK
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "C##LENGUAJES"."FN_CANTIDAD_PRODUCTOS_EN_STOCK" 
RETURN NUMBER IS
  v_total NUMBER;
BEGIN
  SELECT COUNT(*) INTO v_total
  FROM inventario
  WHERE cantidad_disponible > 0;

  RETURN v_total;
END;

/
--------------------------------------------------------
--  DDL for Function FN_CATEGORIA_PRODUCTO
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "C##LENGUAJES"."FN_CATEGORIA_PRODUCTO" (p_id_producto NUMBER)
RETURN VARCHAR2 IS
  v_categoria VARCHAR2(100);
BEGIN
  SELECT c.nombre_categoria INTO v_categoria
  FROM productos p
  JOIN categorias c ON p.id_categoria = c.id_categoria
  WHERE p.id_producto = p_id_producto;

  RETURN v_categoria;
END;

/
--------------------------------------------------------
--  DDL for Function FN_CORREO_CLIENTE
--------------------------------------------------------

  CREATE OR REPLACE EDITIONABLE FUNCTION "C##LENGUAJES"."FN_CORREO_CLIENTE" (p_id_cliente NUMBER)
RETURN VARCHAR2 IS
  v_correo VARCHAR2(100);
BEGIN
  SELECT correo_electronico INTO v_correo FROM clientes WHERE id_cliente = p_id_cliente;
  RETURN v_correo;
END;

/
