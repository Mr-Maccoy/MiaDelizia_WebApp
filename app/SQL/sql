--------------------------------------------------------
--  DDL for View PEDIDOS_ESTADO_TOTAL
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "C##LENGUAJES"."PEDIDOS_ESTADO_TOTAL" ("ID_PEDIDO", "ID_CLIENTE", "FECHA_PEDIDO", "ESTADO_PEDIDO", "MONTO_TOTAL") AS 
  SELECT p.ID_PEDIDO, p.ID_CLIENTE, p.FECHA_PEDIDO, p.ESTADO_PEDIDO, p.MONTO_TOTAL
FROM PEDIDOS p
ORDER BY p.FECHA_PEDIDO DESC
;
--------------------------------------------------------
--  DDL for View PEDIDOS_RECIENTES
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "C##LENGUAJES"."PEDIDOS_RECIENTES" ("ID_PEDIDO", "ID_CLIENTE", "FECHA_PEDIDO", "ESTADO_PEDIDO", "FECHA_ENTREGA", "TIPO_ENVIO", "MONTO_TOTAL") AS 
  SELECT a.ID_PEDIDO, a.ID_CLIENTE, a.FECHA_PEDIDO, a.ESTADO_PEDIDO, a.FECHA_ENTREGA, a.TIPO_ENVIO, a.MONTO_TOTAL
FROM PEDIDOS a
WHERE a.FECHA_PEDIDO >= SYSDATE - 30
ORDER BY a.FECHA_PEDIDO DESC
;
--------------------------------------------------------
--  DDL for View PRODUCTOS_AGOTADOS
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "C##LENGUAJES"."PRODUCTOS_AGOTADOS" ("ID_PRODUCTO", "NOMBRE", "DESCRIPCION", "CANTIDAD_DISPONIBLE") AS 
  SELECT a.ID_PRODUCTO, a.NOMBRE, a.DESCRIPCION, i.CANTIDAD_DISPONIBLE
FROM PRODUCTOS a
JOIN INVENTARIO i ON a.ID_PRODUCTO = i.ID_PRODUCTO
WHERE i.CANTIDAD_DISPONIBLE = 0
;
--------------------------------------------------------
--  DDL for View PRODUCTOS_CATEGORIAS
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "C##LENGUAJES"."PRODUCTOS_CATEGORIAS" ("ID_PRODUCTO", "NOMBRE", "DESCRIPCION", "PRECIO", "DISPONIBILIDAD", "NOMBRE_CATEGORIA") AS 
  SELECT a.ID_PRODUCTO, a.NOMBRE, a.DESCRIPCION, a.PRECIO, a.DISPONIBILIDAD, e.NOMBRE_CATEGORIA
FROM PRODUCTOS a
JOIN CATEGORIAS e ON a.ID_CATEGORIA = e.ID_CATEGORIA
;
--------------------------------------------------------
--  DDL for View USUARIOS_ROLES
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "C##LENGUAJES"."USUARIOS_ROLES" ("ID_USUARIO", "NOMBRE_USUARIO", "EMAIL", "NOMBRE_ROL") AS 
  SELECT u.ID_USUARIO, u.NOMBRE_USUARIO, u.EMAIL, r.NOMBRE_ROL
FROM USUARIOS u
JOIN ROLES r ON u.ID_ROL = r.ID_ROL
;
--------------------------------------------------------
--  DDL for View VENTAS_DIARIAS
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "C##LENGUAJES"."VENTAS_DIARIAS" ("FECHA", "TOTAL_VENTAS") AS 
  SELECT TRUNC(p.FECHA_PEDIDO) AS FECHA, SUM(p.MONTO_TOTAL) AS TOTAL_VENTAS
FROM PEDIDOS p
GROUP BY TRUNC(p.FECHA_PEDIDO)
ORDER BY FECHA DESC
;
--------------------------------------------------------
--  DDL for View V_EVENTOS_PROXIMOS
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "C##LENGUAJES"."V_EVENTOS_PROXIMOS" ("ID_EVENTO", "NOMBRE_EVENTO", "FECHA_EVENTO", "UBICACION") AS 
  SELECT ID_EVENTO, NOMBRE_EVENTO, FECHA_EVENTO, UBICACION
FROM EVENTOS
WHERE FECHA_EVENTO >= SYSDATE
ORDER BY FECHA_EVENTO
;
--------------------------------------------------------
--  DDL for View V_FACTURAS_PAGADAS
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "C##LENGUAJES"."V_FACTURAS_PAGADAS" ("ID_FACTURA", "ID_PEDIDO", "FECHA_FACTURA", "TOTAL") AS 
  SELECT ID_FACTURA, ID_PEDIDO, FECHA_FACTURA, TOTAL
FROM FACTURA
WHERE ESTADO_FACTURA = 'PAGADA'
;
--------------------------------------------------------
--  DDL for View V_HISTORIAL_ESTADO_PEDIDOS
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "C##LENGUAJES"."V_HISTORIAL_ESTADO_PEDIDOS" ("ID_HISTORIAL", "ID_PEDIDO", "CLIENTE", "ESTADO", "FECHA_ESTADO") AS 
  SELECT H.ID_HISTORIAL, P.ID_PEDIDO, C.NOMBRE AS CLIENTE, H.ESTADO, H.FECHA_ESTADO
FROM HISTORIAL_ESTADO_PEDIDO H
JOIN PEDIDOS P ON H.ID_PEDIDO = P.ID_PEDIDO
JOIN CLIENTES C ON P.ID_CLIENTE = C.ID_CLIENTE
;
--------------------------------------------------------
--  DDL for View V_PAGOS_POR_METODO
--------------------------------------------------------

  CREATE OR REPLACE FORCE EDITIONABLE VIEW "C##LENGUAJES"."V_PAGOS_POR_METODO" ("METODO_PAGO", "TOTAL_PAGOS", "MONTO_TOTAL") AS 
  SELECT METODO_PAGO, COUNT(ID_PAGO) AS TOTAL_PAGOS, SUM(MONTO) AS MONTO_TOTAL
FROM PAGOS
WHERE ESTADO_PAGO = 'CONFIRMADO'
GROUP BY METODO_PAGO
;
